{
  "name": "whitesource-renovate",
  "private": true,
  "version": "1.0.0",
  "description": "WhiteSource Renovate",
  "author": "Rhys Arkins <rhys@arkins.net>",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "bundle": "node bin/bundle",
    "eslint": "eslint . --cache --cache-location .cache/eslint",
    "eslint-ci": "eslint . --cache --cache-strategy content --cache-location .cache/eslint --format gha",
    "eslint-debug": "eslint . --debug",
    "eslint-fix": "eslint --debug --cache --cache-location .cache/eslint --fix .",
    "prettier": "prettier --list-different \"**/*.{ts,js,mjs,json,md}\"",
    "prettier-fix": "prettier --write \"**/*.{ts,js,mjs,json,md}\" --end-of-line auto",
    "start": "tsx src/server.js",
    "test": "npm run prettier && npm run eslint && npm run vitest",
    "vitest": "GIT_ALLOW_PROTOCOL=file LOG_LEVEL=fatal vitest run --logHeapUsage",
    "webpack": "NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.701.0",
    "@aws-sdk/lib-storage": "3.535.0",
    "@azure/identity": "4.10.0",
    "@azure/keyvault-secrets": "4.9.0",
    "@sindresorhus/is": "4.6.0",
    "body-parser": "1.20.3",
    "buffer-equal-constant-time": "1.0.1",
    "bunyan": "1.8.15",
    "cron": "1.8.2",
    "delay": "5.0.0",
    "express": "4.21.2",
    "fs-extra": "9.1.0",
    "got": "11.8.6",
    "jsonwebtoken": "9.0.2",
    "minimatch": "3.1.2",
    "nanoid": "5.1.5",
    "parse-link-header": "2.0.0",
    "proper-url-join": "2.1.2",
    "renovate": "41.97.10",
    "safe-stable-stringify": "2.5.0",
    "semver": "7.7.2",
    "split2": "4.2.0",
    "tunnel": "0.0.6",
    "upath": "2.0.1"
  },
  "devDependencies": {
    "@containerbase/eslint-plugin": "1.1.6",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@stylistic/eslint-plugin-js": "4.4.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "1.3.4",
    "callsite": "1.0.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-formatter-gha": "1.6.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-promise": "7.2.1",
    "expect-more-jest": "5.5.0",
    "globals": "16.3.0",
    "jest-extended": "4.0.2",
    "prettier": "2.8.8",
    "tsx": "4.20.4",
    "vitest": "3.2.4",
    "vitest-mock-extended": "3.1.0",
    "webpack": "5.101.3",
    "webpack-cli": "6.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "node": "^22.14.0",
    "npm": "^10.9.0"
  }
}
